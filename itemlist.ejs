  <body>
    <head>
        <%- include('./navbar'); %>
      </head>
    


<main class="container">
    <div>
        <h1>Product List</h1>
        <div class="row">
          <% products.forEach(product => { %>
            <div class="col-md-4 mb-4">
              <div class="card" style="height: 400px;"> <!-- Fixed height for each card -->
                <img class="card-img-top" src="<%= product.image %>" alt="<%= product.name %>" style="height: 300px; object-fit: cover;">
                <div class="card-body d-flex flex-column text-center"> <!-- Added text-center class here -->
                  <h5 class="card-title"><%= product.name %></h5>
                  <p class="card-text flex-grow-1"><%= product.description %></p>
                </div>
              </div>
            </div>
          <% }); %>
        </div>
      </div>
      
      
      

</main>

<footer class="blog-footer">
    <%- include('./footer'); %>
  </footer>


    
